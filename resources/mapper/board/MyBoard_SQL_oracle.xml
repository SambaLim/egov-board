<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="myBoardDAO">
	<select id="myBoardList" resultType="egovframework.com.board.service.MyBoardVO">
	SELECT ID
